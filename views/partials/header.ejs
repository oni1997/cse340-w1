<header>
  <div class="site-identity">
    <h2>Onesmus Auto Shop</h2>
  </div>
  <div class="account-link">
    <!-- Task 1: Show different links based on login status -->
    <% if (locals.loggedin) { %>
      <a href="/account/">Welcome <%= locals.accountData.account_firstname %></a>
      <a href="/account/logout">Logout</a>
    <% } else { %>
      <a href="/account/login">My Account</a>
    <% } %>
  </div>
</header>
